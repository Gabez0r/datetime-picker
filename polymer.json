    "name": "es5",
    "bundle": true,
    "js": {
      "compile": true,
      "minify": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    }
  } ],
  "lint": {
    "rules": [ "polymer-2" ]
  }
}
