  "extraDependencies": [ "bower_components/**/*", "polymer/**/*" ],
  "builds": [ {
    "name": "es6-unbundled",
    "bundle": false,
    "js": {
      "compile": false,
      "minify": true
    },
    "css": {
      "minify": true
    },
    "html": {
